{
  "name": "mbti-contens-platform",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://mbti-platform.shop",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm i @puppeteer/browsers && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "postbuild": "react-snap"
  },
  "dependencies": {
    "antd": "^5.22.6",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-circle-flags": "^0.0.23",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-lottie": "^1.2.10",
    "react-router-dom": "^7.1.0",
    "react-share": "^5.1.2",
    "react-snap-vite": "^1.0.3",
    "vite-plugin-ssr": "^0.4.142"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "vite": "^6.0.3"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--use-gl=egl",
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "skipThirdPartyRequests": true,
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "cacheAjaxRequests": false,
    "source": "dist",
    "concurrency": 1,
    "include": [
      "/personalColor",
      "/personalColor/result/ESTJ",
      "/personalColor/result/ESTP",
      "/personalColor/result/ESFJ",
      "/personalColor/result/ESFP"
    ]
  }
}
